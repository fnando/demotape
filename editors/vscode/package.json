{
  "name": "demotape",
  "displayName": "Demo Tape",
  "description": "Syntax highlighting for DemoTape (.tape) files",
  "version": "0.0.4",
  "publisher": "fnando",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/fnando/demotape"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "files": [
    "*.json",
    "*.png",
    "LICENSE.md",
    "README.md"
  ],
  "categories": [
    "Programming Languages"
  ],
  "devDependencies": {
    "@vscode/vsce": "*"
  },
  "contributes": {
    "languages": [
      {
        "id": "demotape",
        "aliases": [
          "DemoTape",
          "demotape"
        ],
        "extensions": [
          ".tape"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "demotape",
        "scopeName": "source.demotape",
        "path": "./demotape.tmLanguage.json"
      }
    ]
  }
}
